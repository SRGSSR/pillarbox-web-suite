{
  "name": "@srgssr/web-suite-utils",
  "version": "1.0.0",
  "license": "MIT",
  "author": "SRG SSR",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SRGSSR/pillarbox-web-suite.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/SRGSSR/pillarbox-web-suite/tree/main/packages/web-suite-utils#readme",
  "type": "module",
  "main": "dist/web-suite-utils.cjs",
  "browser": "dist/web-suite-utils.umd.min.js",
  "module": "dist/web-suite-utils.js",
  "exports": {
    ".": {
      "import": "./dist/web-suite-utils.js",
      "require": "./dist/web-suite-utils.cjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [
    "video.js",
    "player"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:umd",
    "build:lib": "vite build --config vite.config.lib.js",
    "build:umd": "vite build --config vite.config.umd.js",
    "release:ci": "semantic-release",
    "test": "vitest run --silent --coverage --coverage.reporter text"
  }
}
